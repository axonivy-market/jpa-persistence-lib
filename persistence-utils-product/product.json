{
  "installers": [
    {
      "id": "maven-import",
      "data": {
        "projects": [
          {
            "groupId": "com.axonivy.utils.persistence",
            "artifactId": "persistence-utils-demo",
            "version": "${version}",
            "type": "iar"
          },
          {
            "groupId": "com.axonivy.utils.persistence",
            "artifactId": "persistence-utils-test",
            "version": "${version}",
            "type": "iar",
            "importInWorkspace": "false"
          }
        ],
        "repositories": [
          {
            "id": "axonivy.public",
            "url": "https://maven.axonivy.com",
            "snapshots": {
              "enabled": "true"
            }
          }
        ]
      }
    },
    {
      "id": "maven-dependency",
      "data": {
        "dependencies": [
          {
            "groupId": "com.axonivy.utils.persistence",
            "artifactId": "persistence-utils",
            "version": "${version}",
            "type": "iar"
          }
        ],
        "repositories": [
          {
            "id": "maven.axonivy.com",
            "url": "https://maven.axonivy.com",
            "snapshots": {
              "enabled": "true"
            }
          }
        ]
      }
    }
  ]
}
